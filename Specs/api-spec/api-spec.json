          {
            "request": {
              "method": "GET",
              "uri": "/MULTIPLY?x=4&y=5"
            },
            "expectedResponse": {
              "x-allowExtraHeaders": true,
              "x-bodyMatchMode": "NATIVE",
              "x-arrayOrderedMatching": false,
              "x-arrayCheckCount": false,
              "x-matchResponseSchema": true,
              "statusCode": "200",
              "headers": {},
              "body": "20"
            },
            "x-testShouldPass": true,
            "x-testEnabled": true,
            "x-testName": "multiply",
            "x-testDescription": "Check if multiplication works"
          }
        ]
      }
    }
